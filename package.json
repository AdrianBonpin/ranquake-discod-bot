{
    "name": "tremor-watch",
    "version": "1.2.2",
    "description": "Tremor Watch is a Discord bot that monitors the Philippine Institute of Volcanology and Seismology (PHIVOLCS) for earthquake updates and sends them to a Discord channel.",
    "main": "index.js",
    "scripts": {
        "start": "node --env-file=.env index.js",
        "deploy": "node index.js",
        "test": "node scripts/phivolcs.js",
        "backup:create": "node scripts/backupDb.js create",
        "backup:list": "node scripts/backupDb.js list",
        "backup:clean": "node scripts/backupDb.js clean"
    },
    "keywords": [],
    "author": "Adrian Bonpin",
    "license": "ISC",
    "packageManager": "pnpm@10.19.0",
    "dependencies": {
        "axios": "^1.12.2",
        "better-sqlite3": "^12.4.1",
        "cheerio": "^1.1.2",
        "discord.js": "^14.23.2",
        "groq-sdk": "^0.34.0"
    }
}
