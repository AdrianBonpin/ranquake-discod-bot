{
    "name": "tremor-watch",
    "version": "3.0.0",
    "description": "Tremor Watch is a Discord bot that monitors the Philippine Institute of Volcanology and Seismology (PHIVOLCS) for earthquake updates and sends them to a Discord channel.",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node --env-file=.env dist/index.js",
        "dev": "tsx --env-file=.env src/index.ts",
        "test": "tsx src/scripts/phivolcs.ts",
        "backup:create": "tsx src/scripts/backupDb.ts create",
        "backup:list": "tsx src/scripts/backupDb.ts list",
        "backup:clean": "tsx src/scripts/backupDb.ts clean"
    },
    "keywords": [],
    "author": "Adrian Bonpin",
    "license": "ISC",
    "packageManager": "pnpm@10.19.0",
    "dependencies": {
        "axios": "^1.12.2",
        "cheerio": "^1.1.2",
        "discord.js": "^14.23.2",
        "groq-sdk": "^0.34.0"
    },
    "devDependencies": {
        "@types/node": "^22.10.1",
        "tsx": "^4.19.2",
        "typescript": "^5.7.2"
    }
}
